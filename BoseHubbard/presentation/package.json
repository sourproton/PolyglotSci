{
	"name": "svelte-reveal.js",
	"version": "0.0.10",
	"description": "Reveal.js Svelte wrapper",
	"repository": {
		"type": "git",
		"url": "https://github.com/Ennoriel/svelte-reveal.js"
	},
	"author": "Maxime Dupont <m.dupont103@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev --open --host",
		"build": "vite build",
		"preview": "vite preview --open --host",
		"package": "svelte-kit sync && svelte-package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"prettier": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-static": "^2.0.2",
		"@sveltejs/kit": "^1.5.0",
		"@sveltejs/package": "^1.0.0",
		"@types/node": "^20.1.0",
		"@types/reveal.js": "^4.4.1",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.23",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0",
		"vite-plugin-static-copy": "^0.13.0"
	},
	"dependencies": {
		"ngl": "^0.10.4",
		"plotly.js-dist-min": "^2.22.0",
		"reveal.js": "^4.4.0"
	},
	"type": "module"
}
